  T_electrons = 1.5                     # eV
  nO2 = 0.0                             # m^-3
  nAr = 1.e12                           # m^-3
  ne = 1.e12                            # m^-3
  pAr = 1.0                             # partial pressure (fraction) 
  pO2 = 0.0                             # partial pressure (fraction)
  mO = 16amu                            # kg
  mO2 = 32amu                           # kg
  mO3 = 48amu                           # kg
  mO4 = 64amu                           # kg
  mAr = 40amu                           # kg
  gamma_O = 0.1                         # sticking coefficient
  gamma_O_metastable = 1.0              # sticking coefficient 
  gamma_O2 = 0.007                      # sticking coefficient 
  gamma_O3 = 0.0                        # sticking coefficient 
  gamma_Ar = 1.0                        # sticking coefficient 
end:constants

begin:system
  radius = 0.2                          # m  
  L = 0.2                               # m
  P = 500                               # Watts, W
  h_factor = gudmundsson
  vsheath_solve_method = flux_interpolation
  t_end = 1.0                           # seconds, s
  dt_start = 1.e-12                     # s
  prerun = true 
  total_pressure = 5.0                  # Pa
  plasma_potential_guess = 100          # V

  #power_duty_ratio = 0.1
  #P_shape = square
  #f = 13.56e6                           # Hz
  #P_t_start = 0.0002                    # s
  #T_e_min_eV = 1.5                      # eV
end:system

